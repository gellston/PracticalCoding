<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:PrismExplorer.Views"
                    xmlns:control="clr-namespace:PrismExplorer.Support.UI.Units;assembly=PrismExplorer.Support">

    <Style TargetType="{x:Type views:ExplorerWindowView}"
           BasedOn="{StaticResource {x:Type control:DarkWindow}}">
        <Style.Setters>
            <Setter Property="Content">
                <Setter.Value>
                    <control:FolderTreeView>
                        <control:FolderTreeItem Header="test1"/>
                        <control:FolderTreeItem Header="test2"/>
                        <control:FolderTreeItem Header="test3"/>
                    </control:FolderTreeView>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
</ResourceDictionary>
    
    