<Window x:Class="PrismInspectionExample.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Height="350" Width="525" >
    <DockPanel>
        <UniformGrid Height="40"
                     Columns="4"
                     DockPanel.Dock="Top">
            <Button Content="Inspection"
                    Command="{Binding MainMenuChangedCommand}"
                    CommandParameter="InspectionView"></Button>
            <Button Content="History"
                    Command="{Binding MainMenuChangedCommand}"
                    CommandParameter="InspectionHistoryView"></Button>
        </UniformGrid>

        <ContentControl prism:RegionManager.RegionName="Main"/>
    </DockPanel>
</Window>
