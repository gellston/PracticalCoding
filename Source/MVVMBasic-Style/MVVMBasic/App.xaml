<Application x:Class="MVVMBasic.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MVVMBasic"
             xmlns:viewmodel="clr-namespace:MVVMBasic.ViewModel"
             StartupUri="View/MainWindowView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/MainWindowStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <viewmodel:ViewModelLocator x:Key="Locator"></viewmodel:ViewModelLocator>
        </ResourceDictionary>
       
    </Application.Resources>
</Application>
